{
  "name": "kinvey-cli",
  "version": "4.0.0-beta.1",
  "description": "Utility for deploying and managing FlexServices on Kinvey's FlexService Runtime",
  "keywords": [
    "Kinvey"
  ],
  "homepage": "http://www.kinvey.com",
  "bugs": "https://github.com/Kinvey/kinvey-cli/issues",
  "license": "Apache-2.0",
  "author": "Kinvey, Inc.",
  "contributors": [
    "Mark van Seventer <mark@kinvey.com>",
    "Sam Levin <sam@kinvey.com>"
  ],
  "bin": {
    "kinvey": "./bin/kinvey"
  },
  "repository": {
    "type": "git",
    "url": "Kinvey/kinvey-cli"
  },
  "scripts": {
    "start": "./bin/kinvey",
    "prepublish": "./node_modules/.bin/dos2unix ./bin/kinvey",
    "pretest": "npm run eslint",
    "lint": "npm run eslint",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "./node_modules/.bin/mocha test/unit --NODE_CONFIG_DIR=./test/config",
    "test-integration": "./node_modules/.bin/mocha test/integration",
    "coverage": "./node_modules/.bin/nyc --reporter=html --reporter=text ./node_modules/.bin/mocha test --NODE_CONFIG_DIR=./test/config",
    "eslint": "./node_modules/.bin/eslint bin config lib test"
  },
  "dependencies": {
    "aes-js": "3.1.1",
    "archiver": "1.0.1",
    "async": "1.4.2",
    "chalk": "2.1.0",
    "config": "1.16.0",
    "console.table": "0.10.0",
    "inquirer": "0.12.0",
    "isemail": "1.2.0",
    "joi": "13.3.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "lodash.merge": "4.6.0",
    "lodash.sortby": "4.7.0",
    "moment": "2.22.1",
    "request": "2.87.0",
    "semver": "5.5.0",
    "update-notifier": "0.5.0",
    "uuid": "2.0.3",
    "uuid-v4": "0.1.0",
    "valid-url": "1.0.9",
    "winston": "2.4.0",
    "yargs": "10.0.3"
  },
  "devDependencies": {
    "body-parser": "1.18.2",
    "chai": "3.3.0",
    "dos2unix-cli": "1.0.1",
    "eslint": "4.17.0",
    "eslint-config-kinvey-platform": "kinvey/eslint-kinvey-platform#v0.1.1",
    "eslint-plugin-import": "2.8.0",
    "express": "4.16.2",
    "mocha": "5.0.4",
    "nyc": "11.1.0",
    "sinon": "4.1.3",
    "sinon-chai": "2.14.0",
    "snap-shot-it": "4.0.1",
    "strip-ansi": "4.0.0",
    "suppose": "0.6.2",
    "test-console": "1.0.0"
  },
  "engines": {
    "node": ">=6.x.x"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "preferGlobal": true,
  "yargs": {
    "boolean-negation": false,
    "camel-case-expansion": false
  }
}
